{
	"name": "ink-js-console",
	"version": "0.0.0",
	"description": "Javascript based ink-console.",
	"license": "MIT",
	"repository": "icyJoseph/ink-js-console",
	"author": {
		"name": "icyJoseph",
		"email": "sephxd1234@gmail.com",
		"url": "https://icjoseph.com"
	},
	"main": "dist/index.js",
	"engines": {
		"node": ">=6"
	},
	"scripts": {
		"pretest": "npm run build",
		"test": "xo && nyc ava",
		"test:quick": "nyc ava",
		"build": "babel src --out-dir=dist",
		"prepublish": "npm run build"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"ink",
		"ink-component",
		"ink-console",
		"javascript-ink-console",
		"interactive"
	],
	"dependencies": {
		"chalk": "^2.4.1",
		"indent-string": "^3.2.0",
		"ink": "^0.5.1",
		"prop-types": "^15.5.10"
	},
	"devDependencies": {
		"ava": "^0.20.0",
		"babel-cli": "^6.24.1",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-plugin-transform-react-jsx": "^6.24.1",
		"eslint-config-xo-react": "^0.13.0",
		"eslint-plugin-react": "^7.1.0",
		"nyc": "^11.0.0",
		"xo": "^0.18.2"
	},
	"babel": {
		"plugins": [
			"transform-object-rest-spread",
			[
				"transform-react-jsx",
				{
					"pragma": "h",
					"useBuiltIns": true
				}
			]
		]
	},
	"ava": {
		"babel": {
			"presets": [
				"@ava/stage-4"
			],
			"plugins": [
				[
					"transform-react-jsx",
					{
						"pragma": "h",
						"useBuiltIns": true
					}
				]
			]
		}
	},
	"xo": {
		"extends": [
			"xo-react"
		],
		"rules": {
			"react/no-unused-prop-types": 1,
			"unicorn/filename-case": [
				"error",
				"camelCase",
				"pascalCase"
			],
			"object-curly-spacing": [
				0,
				"always"
			],
			"react/jsx-tag-spacing": [
				0,
				"always"
			],
			"react/no-deprecated": [
				0,
				"always"
			],
			"operator-linebreak": [
				0
			]
		},
		"settings": {
			"react": {
				"pragma": "h"
			}
		},
		"prettier": true
	},
	"nyc": {
		"reporter": [
			"html",
			"text"
		]
	}
}
